{
    "swagger": "2.0",
    "info": {
        "title": "goLog service's API",
        "contact": {},
        "license": {},
        "version": "v1"
    },
    "basePath": "/api/v1",
    "paths": {
        "/admin": {
            "post": {
                "description": "登录验证，获取 token 令牌",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "管理员登录",
                "parameters": [
                    {
                        "type": "string",
                        "description": "管理员",
                        "name": "name",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "管理员密码",
                        "name": "password",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "token……",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "invalid params or the name and password is no correct",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "no exist",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "the server has error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}